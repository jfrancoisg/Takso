        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noEmitOnError": false,
        "noUncheckedIndexedAccess": true,
        "outDir": "../public/assets/js",
        "removeComments": true,
        "strict": true,
        "target": "ESNext"
    },
    "include": [
        "../public/assets/ts-scss/ts/**/*"
    ]
}